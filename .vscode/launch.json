{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug",
            "cwd": "${workspaceRoot}",
            "executable": "./output/main.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "interface": "swd",
            "device": "STM32F407VG",
            "runToEntryPoint": "main",
            "showDevDebugOutput": "raw",
            "svdFile": "${workspaceRoot}/.vscode/STM32F407.svd",
            "configFiles": [
                "interface/stlink-v2-1.cfg",
                "target/stm32f4x.cfg"
            ],
            // "preLaunchTask": "${defaultBuildTask}",
            "liveWatch": {
                "enabled": true,
                "samplesPerSecond": 4
            }
            // "swoConfig": {"swoFrequency": 1800000}
        },
        {
            "name": "Run",
            "cwd": "${workspaceRoot}",
            "executable": "./output/main.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "interface": "swd",
            "device": "STM32F407VG",
            "runToEntryPoint": "main",
            "showDevDebugOutput": "raw",
            "svdFile": "${workspaceRoot}/.vscode/STM32F407.svd",
            "configFiles": [
                "interface/stlink-v2-1.cfg",
                "target/stm32f4x.cfg"
            ],
            // "preLaunchTask": "${defaultBuildTask}",
            "postLaunchCommands": [
                "monitor reset run",
                "detach"
            ]
        }
        // ,
        // {
        //     "showDevDebugOutput": "parsed",
        //     "cwd": "${workspaceRoot}",
        //     "executable": "./build/STM32CubeMX.elf",
        //     "name": "Debug STM32",
